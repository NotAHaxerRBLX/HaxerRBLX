-- Scripts:

local function OFHJFW_fake_script() -- MinimizeButton.LocalScript 
	local script = Instance.new('LocalScript', MinimizeButton)

	local Bg = script.Parent.Parent.Parent.Bg
	
	local closed = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if closed == false then
			closed = true
			Bg.Visible = false
		else
			closed = false
			Bg.Visible = true
		end
	end)
end
coroutine.wrap(OFHJFW_fake_script)()
local function EZZOTG_fake_script() -- Text_2.LocalScript 
	local script = Instance.new('LocalScript', Text_2)

	local Line1 = script.Parent.Parent.Line1
	local Line2 = script.Parent.Parent.Line2
	
	local TweenService = game:GetService("TweenService")
	
	local TextTweenOut = TweenService:Create(script.Parent, TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), { TextTransparency = 1 })
	local BackgroundTweenOut = TweenService:Create(script.Parent.Parent, TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), { BackgroundTransparency = 1 })
	
	local Line1TweenIn = TweenService:Create(Line1, TweenInfo.new(2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), { Size = UDim2.new(-0.618, 0, 0.035, 0) })
	local Line2TweenIn = TweenService:Create(Line2, TweenInfo.new(2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), { Size = UDim2.new(-0.618, 0, 0.035, 0) })
	
	local Line1TweenOut = TweenService:Create(Line1, TweenInfo.new(1, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), { BackgroundTransparency = 1 })
	local Line2TweenOut = TweenService:Create(Line2, TweenInfo.new(1, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), { BackgroundTransparency = 1 })
	
	if game:IsLoaded() then
		task.wait(2)
		Line1TweenIn:Play()
		Line2TweenIn:Play()
		Line2TweenIn.Completed:Wait()
		task.wait(2)
		TextTweenOut:Play()
		BackgroundTweenOut:Play()
		Line1TweenOut:Play()
		Line2TweenOut:Play()
		TextTweenOut.Completed:Wait()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.MainScreen.Visible = true
	end
end
coroutine.wrap(EZZOTG_fake_script)()
local function EVAYB_fake_script() -- Text_3.LocalScript 
	local script = Instance.new('LocalScript', Text_3)

	local player = game.Players.LocalPlayer
	
	script.Parent.Text = "Welcome, " .. player.Name .. "."
end
coroutine.wrap(EVAYB_fake_script)()
local function GXDHYV_fake_script() -- HacksScreen.LocalScript 
	local script = Instance.new('LocalScript', HacksScreen)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.HacksScreen.Visible = true
	end)
end
coroutine.wrap(GXDHYV_fake_script)()
local function VMCJI_fake_script() -- GameScreen.LocalScript 
	local script = Instance.new('LocalScript', GameScreen)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.GameViewerScreen.Visible = true
		
		print("==Workspace==")
		
		for i, v in pairs(game.Workspace:GetDescendants()) do
			
			print(v.Name .. ":" .. v.ClassName)
		end
		
		print("==ReplicatedStorage==")
	
		for i, v in pairs(game.ReplicatedStorage:GetDescendants()) do
	
			print(v.Name .. ":" .. v.ClassName)
		end
		
		task.wait(2)
		script.Parent.Parent.Visible = true
		script.Parent.Parent.Parent.GameViewerScreen.Visible = false
	end)
end
coroutine.wrap(VMCJI_fake_script)()
local function ZLAC_fake_script() -- WalkspeedButton.LocalScript 
	local script = Instance.new('LocalScript', WalkspeedButton)

	local player = game.Players.LocalPlayer
	
	local TextBox = script.Parent.TextBox
	
	local LoopedButton = script.Parent.LoopedButton
	
	local opened = false
	
	local looped = false
	
	LoopedButton.MouseButton1Click:Connect(function()
		if looped == false then
			looped = true
		else
			looped = false
		end
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		if opened == false then
			opened = true
			script.Parent.UIStroke.Enabled = true
			TextBox.Visible = true
			LoopedButton.Visible = true
		else
			opened = false
			script.Parent.UIStroke.Enabled = false
			TextBox.Visible = false
			LoopedButton.Visible = false
		end
	end)
	
	script.Parent.MouseButton2Click:Connect(function()
		player.Character:WaitForChild("Humanoid").WalkSpeed = 16
		looped = false
		TextBox.Text = "16"
		LoopedButton.UIStroke.Enabled = false
		script.Parent.UIStroke.Enabled = false
		LoopedButton.Visible = false
		TextBox.Visible = false
		opened = false
		script.Parent.Text = "Resetted WalkSpeed"
		task.wait(1)
		script.Parent.Text = "Walkspeed (Default is 16)"
	end)
	
	TextBox:GetPropertyChangedSignal("Text"):Connect(function()
		TextBox.Text = TextBox.Text:gsub('%D+', '');
	end)
	
	TextBox.FocusLost:Connect(function()
		if looped == false then
			local character = player.Character
			if character then
				character:FindFirstChild("Humanoid").WalkSpeed = TextBox.Text
			end
		elseif looped == true then
			while looped == true do
				local character = player.Character
				if character then
					character:FindFirstChild("Humanoid").WalkSpeed = TextBox.Text
				end
				task.wait()
			end
		end
	end)
end
coroutine.wrap(ZLAC_fake_script)()
local function ANWGYU_fake_script() -- LoopedButton.LocalScript 
	local script = Instance.new('LocalScript', LoopedButton)

	local opened = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if opened == false then
			opened = true
			script.Parent.UIStroke.Enabled = true
		else
			opened = false
			script.Parent.UIStroke.Enabled = false
		end
	end)
end
coroutine.wrap(ANWGYU_fake_script)()
local function SOXIMEQ_fake_script() -- JumpPowerButton.LocalScript 
	local script = Instance.new('LocalScript', JumpPowerButton)

	local player = game.Players.LocalPlayer
	
	local TextBox = script.Parent.TextBox
	
	local LoopedButton = script.Parent.LoopedButton
	
	local opened = false
	
	local looped = false
	
	LoopedButton.MouseButton1Click:Connect(function()
		if looped == false then
			looped = true
		else
			looped = false
		end
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		if opened == false then
			opened = true
			script.Parent.UIStroke.Enabled = true
			TextBox.Visible = true
			LoopedButton.Visible = true
		else
			opened = false
			script.Parent.UIStroke.Enabled = false
			TextBox.Visible = false
			LoopedButton.Visible = false
		end
	end)
	
	script.Parent.MouseButton2Click:Connect(function()
		local character = player.Character
		if character:FindFirstChild("Humanoid").UseJumpPower == true then
			character:FindFirstChild("Humanoid").JumpPower = TextBox.Text
			looped = false
			TextBox.Text = "50"
			LoopedButton.UIStroke.Enabled = false
			script.Parent.UIStroke.Enabled = false
			LoopedButton.Visible = false
			TextBox.Visible = false
			opened = false
			script.Parent.Text = "Resetted JumpPower"
			task.wait(1)
			script.Parent.Text = "Jump Power (Default is 50)"
		end
	end)
	
	TextBox:GetPropertyChangedSignal("Text"):Connect(function()
		TextBox.Text = TextBox.Text:gsub('%D+', '');
	end)
	
	TextBox.FocusLost:Connect(function()
		if looped == false then
			local character = player.Character
			if character then
				if character:FindFirstChild("Humanoid").UseJumpPower == true then
					character:FindFirstChild("Humanoid").JumpPower = TextBox.Text
				else
					script.Parent.Text = "This Game uses JumpHeight. Please use the JumpHeight Button."
					task.wait(1)
					script.Parent.Text = "Jump Power (Default is 50)"
				end
			end
		elseif looped == true then
			while looped == true do
				local character = player.Character
				if character then
					character:FindFirstChild("Humanoid").JumpPower = TextBox.Text
				end
				task.wait()
			end
		end
	end)
end
coroutine.wrap(SOXIMEQ_fake_script)()
local function VVFFVE_fake_script() -- LoopedButton_2.LocalScript 
	local script = Instance.new('LocalScript', LoopedButton_2)

	local opened = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if opened == false then
			opened = true
			script.Parent.UIStroke.Enabled = true
		else
			opened = false
			script.Parent.UIStroke.Enabled = false
		end
	end)
end
coroutine.wrap(VVFFVE_fake_script)()
